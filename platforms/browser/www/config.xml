<?xml version='1.0' encoding='utf-8'?>
<widget id="net.hebasoft.pexesowildanimalspro" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pairs Animals Pro</name>
    <description>
        Pairs, is a card game which is good for memory training or just for fun
    </description>
    <author email="michhory@gmail.com" href="http://hebasoft.net">
        HeBaSoft
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
</widget>
