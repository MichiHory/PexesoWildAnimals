<?xml version='1.0' encoding='utf-8'?>
<widget id="net.hebasoft.pexesowildanimals" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pexeso Animals</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="michhory@gmail.com" href="http://hebasoft.net">
        HeBaSoft
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
